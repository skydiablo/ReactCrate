{
    "name": "skydiablo/react-crate",
    "type": "library",
    "license": "MIT",
    "autoload": {
        "psr-4": {
            "SkyDiablo\\ReactCrate\\": "src/"
        }
    },
    "authors": [
        {
            "name": "SkyDiablo",
            "email": "skydiablo@gmx.net"
        }
    ],
    "require": {
        "react/http": "^3@dev"
    },
    "suggest": {
        "doctrine/dbal": "Required for using the Doctrine DBAL Driver integration with CrateDB.",
        "react/async": "Required for synchronous execution in the Doctrine DBAL Driver."
    },
    "require-dev": {
        "phpunit/phpunit": "^10.1"
    },
    "autoload-dev": {
        "psr-4": {
            "SkyDiablo\\ReactCrate\\Tests\\": "tests/"
        }
    }
}
